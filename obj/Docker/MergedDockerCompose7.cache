name: dockercompose15649418047798558452
services:
  mysqldb:
    environment:
      MYSQL_DATABASE: redisdemo
      MYSQL_PASSWORD: password
      MYSQL_ROOT_PASSWORD: password
      MYSQL_USER: felipe
    image: mysql:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "3307"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Users\Felipe\source\repos\RedisDemo3\store
        target: /var/lib/mysql
